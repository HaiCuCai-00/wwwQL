@model BSAF16072021.Models.Training

@{
    ViewBag.Title = "Information of the training staff";
    Layout = "~/Areas/admin/Views/Shared/_AdminLayout.cshtml";
}

<h4><center>Information of the training staff</center></h4>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Training username
        </dt>

        <dd>
            @Html.DisplayFor(model => model.training_name)
        </dd>

        <dt>
            Training password
        </dt>

        <dd>
            @Html.DisplayFor(model => model.training_password)
        </dd>

        <dt>
            Trainingemail
        </dt>

        <dd>
            @Html.DisplayFor(model => model.training_email)
        </dd>

        <dt>
            Telephone
        </dt>

        <dd>
            @Html.DisplayFor(model => model.training_telephone)
        </dd>

        <dt>
            Address
        </dt>

        <dd>
            @Html.DisplayFor(model => model.training_address)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.training_id }, new { @class = "btn btn-xs btn-warning" })
    @Html.ActionLink("Back to List", "Index")
</p>
